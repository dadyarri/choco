{
  "name": "choco-client",
  "version": "1.5.0",
  "private": false,
  "repository": {
    "type": "github",
    "url": "https://github.com/dadyarri/choco"
  },
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/react": "^2.4.9",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@here/maps-api-for-javascript": "^1.38.0",

    "axios": "^1.2.4",
    "chart.js": "^4.2.1",
    "chroma-js": "^2.4.2",
    "formik": "^2.2.9",
    "framer-motion": "^8.5.4",
    "http-proxy-middleware": "^2.0.6",
    "luxon": "^3.2.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.7.0",
    "react-spinners": "^0.13.8",
    "react-toastify": "^9.1.1",
    "rimraf": "^4.1.2",
    "uuid": "^9.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.4.0",
    "@types/luxon": "^3.2.0",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-react": "^3.1.0",
    "typescript": "^4.9.4",
    "vite": "^4.2.1",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.8"
  },
  "scripts": {
    "prestart": "node aspnetcore-https && node aspnetcore-react",
    "start": "rimraf ./build && vite",
    "build": "tsc && vite build",
    "lint": "eslint ./src/",
    "serve": "vite preview"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "react-app",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "@typescript-eslint/no-non-null-assertion": "off"
    },
    "ignorePatterns": [
      "aspnetcore*.js",
      "src/setupProxy.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
